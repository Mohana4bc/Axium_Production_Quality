					<!--	<Image width="219px" height="40px" src="images/Axium_Logo.png"></Image>-->
					<!--</VBox>-->
					<VBox height="30%" alignItems="Center"/>
					<VBox alignItems="Center">
						<!--<Button text="Finished Goods Put Away" iconDensityAware="false" visible="true" enabled="true" width="219px" iconFirst="true"-->
						<!--	type="Emphasized" press="onPressFG" icon="sap-icon://order-status"/>-->
						<!--<Button text="RM Pick" iconDensityAware="false" visible="true" enabled="true" width="219px" iconFirst="true" type="Emphasized"-->
